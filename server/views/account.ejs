    </div>
</div>

<script>
    var prieteni = '<%- JSON.stringify(friends) %>';
    var ut_name = '<%- JSON.stringify(user.username) %>'
    var ut_email = '<%- JSON.stringify(user.email) %>'
    var ut_id = '<%- JSON.stringify(user.id) %>'
    var ut_incall = '<%- JSON.stringify(user.incall) %>'
</script>

<script src="javascript/account.js">
</script>

